{
    "compilerOptions": {
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "baseUrl": "src",
      "outDir": "dist",
      "target": "ES6",
      "sourceMap": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "allowImportingTsExtensions": true,
      "noEmit": true,
    },
    "include": ["src/**/*"]
  }
  